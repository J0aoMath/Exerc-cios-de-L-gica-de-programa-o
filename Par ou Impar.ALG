algoritmo "ParOuImpar"
var
Numero: Real
Par, Impar: Caractere
inicio
      Escreval ("Escreva um número.")
      Leia (Numero)
           Se Numero % 2 = 0 Entao
              Escreva ("O número ", Numero, " é Par.")
           Senao
              Escreva ("O número ", Numero, " é Impar.")
           FIMSE

fimalgoritmo