algoritmo "LerMatriz"
var
Mat: vetor[1..3,1..2] de inteiro
l, c: inteiro
inicio
  Para l <- 1 ate 3 faca
     Para c <- 1 ate 2 faca
          Escreva ("Digite o valor da posição ","[",l,",",c,"]: ")
          Leia(Mat[l,c])
     FimPara
  FimPara

  Para l <- 1 ate 3 faca
       Para c <- 1 ate 2 faca
            Escreva(mat[l,c]:5)
       FimPara
       EscrevaL("  ")
  FimPara


fimalgoritmo